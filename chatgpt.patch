+            window.katex.render(latex, el, {
+              throwOnError: false,
+            });
+            el.setAttribute('role', 'math');
+            el.setAttribute('aria-label', latex);
+          } catch (error) {
+            console.error('KaTeX render failed for expression:', latex, error);
+          }
+        });
+      });
